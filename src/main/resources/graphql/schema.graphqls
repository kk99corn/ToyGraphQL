type Query {
    findAll: [Person]
    findById(id: ID!): Person
    findAddressAll: [Address]
}

type Mutation {
    person(
        id: Int
        firstName: String
        lastName: String
        phoneNumber: String
        email: String
        addressId: Int
    ): Person
    address(
        id: Int
        address: String
        city: String
        state: String
        zip: String
    ): Address
}

type Person {
    id: ID!
    firstName: String
    lastName: String
    phoneNumber: String
    email: String
    address: Address
}

type Address {
    id: ID!
    address: String
    city: String
    state: String
    zip: String
}